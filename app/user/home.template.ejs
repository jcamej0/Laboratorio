        <span>Laboratorie</span>
      </h2>
      <span flex></span>
      <md-menu>
        <md-button aria-label="Menu usuario" class="md-icon-button" ng-click="homeCtrl.openMenu($mdOpenMenu, $event)">
          <md-icon md-menu-origin md-svg-icon="/bower_components/material-design-icons/action/svg/production/ic_face_48px.svg"></md-icon>
        </md-button>
        <md-menu-content width="4">
          <md-menu-item>
            <md-button aria-label="Configuracion" ng-click="homeCtrl.openBottomSheet()">
              <md-icon md-svg-icon="/bower_components/material-design-icons/action/svg/production/ic_settings_48px.svg"></md-icon>
              Configuracion
            </md-button>
          </md-menu-item>
          <md-menu-item>
            <md-button aria-label="Editar Usuario" ng-href="home/editUser">
              <md-icon md-svg-icon="/bower_components/material-design-icons/content/svg/production/ic_create_48px.svg"></md-icon>
