{
  "name": "laboratoire",
  "version": "0.0.1",
  "description": "Veterinarian lab based on Node.js and Mongo, with swagger-node",
  "main": "index.js",
  "scripts": {
    "run": "nodemon -e js,ejs,json,yaml",
    "run-develop": "npm run run & npm run develop",
    "debug": "DEBUG=* npm run run",
    "seed": "node api/seeds/index.js",
    "docs": "node doc.js",
    "install": "node doc.js -i && node build && node manage-oauth.js && npm run email-build && npm run seed && bower install && npm run build",
    "lint": "eslint . --ext .js",
    "lint-fix": "eslint --fix . --ext .js",
    "email-start": "gulp",
    "email-build": "node build && PROD=true gulp build",
    "develop": "gulp dev-assets",
    "build": "PROD=true gulp build-assets",
    "test": "node api/mocks/cache.js && cucumber.js test/features/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/gustavogelf/laboratoire.git"
  },
  "engines": {
    "node": ">=4.0"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://bitbucket.org/gustavogelf/laboratoire#readme",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.1",
    "canis-familiaris": "^1.5.0",
    "debug": "^2.2.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "handlebars": "^4.0.5",
    "helmet": "^2.1.1",
    "json-schema-ref-parser": "^3.1.2",
    "jspm": "^0.16.39",
    "lodash": "^4.14.1",
    "mailgun-js": "^0.7.12",
    "mockaroo": "^0.1.7",
    "mongoose": "^4.6.0",
    "swagger-express-mw": "^0.1.0",
    "volos-management-redis": "^0.9.10",
    "volos-oauth-redis": "^0.13.0",
    "volos-swagger-oauth": "0.0.2",
    "yamljs": "^0.2.8"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "browser-sync": "^2.11.0",
    "cors": "^2.7.1",
    "cucumber": "^2.0.0-rc.7",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.3.1",
    "gulp": "git+https://github.com/gulpjs/gulp.git#4.0",
    "gulp-cli": "^1.1.0",
    "gulp-htmlmin": "^1.1.1",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^2.4.0",
    "gulp-include": "^2.3.1",
    "gulp-inline-css": "^3.0.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "inky": "^1.3.6",
    "lazypipe": "^1.0.1",
    "nodemon": "^1.10.0",
    "open": "0.0.5",
    "panini": "^1.3.0",
    "rimraf": "^2.3.3",
    "shelljs": "^0.7.3",
    "should": "^11.2.0",
    "siphon-media-query": "^1.0.0",
    "supertest": "^3.0.0",
    "swagger-ui": "^2.1.5",
    "watch": "^0.19.2",
    "yargs": "^4.1.0"
  }
}
